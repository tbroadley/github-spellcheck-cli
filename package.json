{
  "name": "spellchecker-cli",
  "version": "2.0.0",
  "description": "A command-line tool for spellchecking files.",
  "main": "index.js",
  "bin": {
    "spellchecker": "./index.js"
  },
  "scripts": {
    "lint": "eslint \"**/*.js\"",
    "spellchecker": "node ./index.js --files \"**/*.md\" --dictionary dictionary.txt"
  },
